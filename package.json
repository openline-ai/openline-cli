{
  "name": "openline-cli",
  "version": "1.0.9",
  "description": "CLI for the Openline ecosystem",
  "main": "./build/cli.js",
  "bin": {
    "openline": "./build/cli.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/cli.ts",
    "start:windows": "nodemon --watch 'src/**/*.ts' --exec \"npx ts-node\" src/cli.ts",
    "create": "npm run build",
    "build": "tsc -p .",
    "local": "sudo npm i -g && openline",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "targets": [ "node16-linux-arm64", "node16-macos-arm64"],
    "outputPath": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openline-ai/openline-cli.git"
  },
  "author": "mattbr0wn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openline-ai/openline-cli/issues"
  },
  "homepage": "https://github.com/openline-ai/openline-cli#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^9.4.1",
    "figlet": "^1.5.2",
    "path": "^0.12.7",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/shelljs": "^0.8.11",
    "nodemon": "^2.0.20",
    "pkg": "^5.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
